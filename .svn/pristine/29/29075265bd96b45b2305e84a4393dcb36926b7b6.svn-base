                    android:text="Support Application"
                    android:textAlignment="gravity"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_twenty_two_dp"
                    android:visibility="gone" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/lay_share"
                        android:layout_width="@dimen/dimen_category_width"
                        android:layout_height="wrap_content"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dimen_image_height"
                            android:layout_height="@dimen/dimen_image_height"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/dimen_padding_imagelogo"
                            android:background="@drawable/share"
                            android:backgroundTint="@color/theme_color"
                            android:contentDescription="@null"
                            android:layerType="software"
                            tools:ignore="VectorDrawableCompat" />

                        <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen_four_dp"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:maxLength="20"
                            android:maxLines="2"
                            android:text="Share App"
                            android:textAlignment="gravity"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen_title_text"
                            android:textStyle="normal" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lay_rate"
                        android:layout_width="@dimen/dimen_category_width"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_four_dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dimen_image_height"
                            android:layout_height="@dimen/dimen_image_height"
                            android:layout_gravity="center"
                            android:layout_margin="@dimen/dimen_padding_imagelogo"
                            android:background="@drawable/rating"
                            android:backgroundTint="@color/theme_color"
                            android:contentDescription="@null"
                            android:layerType="software"
                            tools:ignore="VectorDrawableCompat" />

                        <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/dimen_four_dp"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:maxLength="20"
                            android:maxLines="2"
                            android:text="Rate us"
                            android:textAlignment="gravity"
                            android:textColor="@color/black"
                            android:textSize="@dimen/dimen_title_text"
                            android:textStyle="normal" />

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>


            <Button
                android:id="@+id/btn_logout"
                style="?attr/materialButtonOutlinedStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen_eight_dp"
                android:layout_marginRight="@dimen/dimen_eight_dp"
                android:backgroundTint="@color/theme_color"
                android:letterSpacing="0"
                android:text="Logout"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="@dimen/dimen_header_text" />

            <com.nebulacompanies.ibo.ecom.utils.MyTextViewEcom
                android:id="@+id/tv_version"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="@dimen/dimen_eight_dp"
                android:text=""
                android:textAllCaps="true"
                android:textColor="@color/theme_color"
                android:textSize="@dimen/dimen_title_text" />
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>

